--><config>
    <modules>
        <Vaimo_UploadLogo>
            <version>0.1.5</version>
        </Vaimo_UploadLogo>
    </modules>
	<global>
		<helpers>
			<uploadlogo>
				<class>Vaimo_UploadLogo_Helper</class>
			</uploadlogo>
		</helpers>
        <blocks>
            <vaimo_uploadlogo>
               	<class>Vaimo_UploadLogo_Block</class>
            </vaimo_uploadlogo>
            <page>
   				<rewrite>
   					<html_header>Vaimo_UploadLogo_Block_Page_Html_Header</html_header>
   				</rewrite>
   			</page>
        </blocks>
	</global>
    <adminhtml>
        <translate>
           <modules>
               <Vaimo_UploadLogo>
                   <files>
                       <uploadlogo>Vaimo_UploadLogo.csv</uploadlogo>
                   </files>
               </Vaimo_UploadLogo>
           </modules>
        </translate>
    </adminhtml>
</config>
