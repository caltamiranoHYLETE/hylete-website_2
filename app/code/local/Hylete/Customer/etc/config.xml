<?xml version="1.0"?>
<config>
    <modules>
        <Hylete_Customer>
            <version>0.0.1</version>
        </Hylete_Customer>
    </modules>
    <global>
        <models>
            <customermodule>
                <class>Hylete_Customer_Model</class>
            </customermodule>
            <customer>
                <rewrite>
                    <customer>Hylete_Customer_Model_Customer</customer>
                </rewrite>
            </customer>
        </models>
    </global>
    <frontend>
        <routers>
            <customer>
                <args>
                    <modules>
                        <Hylete_Customer before="Mage_Customer">Hylete_Customer</Hylete_Customer>
                    </modules>
                </args>
            </customer>
        </routers>
    </frontend>
</config>