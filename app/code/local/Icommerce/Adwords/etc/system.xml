<?xml version="1.0"?>
<config>
    <tabs>
        <vaimo_addons translate="label" module="adwords">
            <label><![CDATA[<div style="background: url(data:image/png;base64,) no-repeat; height: 18px; display: inline-block; padding: 1px 0 0 23px; margin-top: 2px;">Vaimo Modules</div>]]></label>
            <sort_order>999</sort_order>
        </vaimo_addons>
    </tabs>
    <sections>
        <adwords translate="label comment" module="adwords">
            <tab>vaimo_addons</tab>
            <label>Adwords</label>
            <frontend_type>text</frontend_type>
            <sort_order>010040</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <settings translate="label comment" module="adwords">
                    <label>Settings</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>2</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
			            <active translate="label">
			              <label>Enabled</label>
			              <frontend_type>select</frontend_type>
			              <source_model>adminhtml/system_config_source_yesno</source_model>
			              <sort_order>1</sort_order>
			              <show_in_default>1</show_in_default>
			              <show_in_website>1</show_in_website>
			              <show_in_store>1</show_in_store>
			            </active>
			            <google_conversion_id translate="label">
                            <label>Google conversion id</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
                        </google_conversion_id>
                        <google_conversion_language translate="label">
                            <label>Google conversion language</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>3</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
                        </google_conversion_language>
                        <google_conversion_format translate="label">
                            <label>Google conversion format</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>4</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
                        </google_conversion_format>
                        <google_conversion_color translate="label">
                            <label>Google conversion color</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>5</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
                        </google_conversion_color>
                        <google_conversion_label translate="label">
                            <label>Google conversion label</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>6</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
                        </google_conversion_label>
                        <google_conversion_value translate="label">
                            <label>Google conversion value</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>7</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
                        </google_conversion_value>
                        <google_remarketing_active translate="label">
                            <label>Activate Google remarketing</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>9</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </google_remarketing_active>
                        <google_remarketing_vertical translate="label comment">
                            <depends><google_remarketing_active>1</google_remarketing_active></depends>
                            <label>Vertical Type</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adwords/adminhtml_system_config_source_verticals</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </google_remarketing_vertical>
                        <google_remarketing_only translate="label">
                            <depends><google_remarketing_active>1</google_remarketing_active></depends>
                            <label>Google remarketing only</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>11</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </google_remarketing_only>
                        <google_tag_params translate="label comment">
                            <depends><google_remarketing_active>1</google_remarketing_active></depends>
                            <label>Google Tag Params</label>
                            <frontend_model>adwords/adminhtml_form_field_googletagparams</frontend_model>
                            <backend_model>adwords/adminhtml_system_config_backend_googletagparams</backend_model>
                            <sort_order>12</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </google_tag_params>
                    </fields>
                </settings>
        	</groups>
        </adwords>
    </sections>
</config>
